<?xml version="1.0"?>

<project name="getEclipseClasspath-test" basedir="." default="foo">

	<!-- definition of condition HasNature -->
	<typedef name="getEclipseClasspath"
	         classname="org.ant4eclipse.jdt.ant.GetEclipseClassPathTask"
	/>
	
	<target name="getEclipseClasspath">
		<getEclipseClasspath workspace="${workspaceDir}"
			pathSeparator="${pathSeparator}"
			runtime="${runtimeClasspath}"
			projectName="${projectName}"
			property="classpath"/>
	</target>
	
	


	<!-- default dummy target -->
	<target name="foo" />
</project>